#include <stdlib.h>
#include <stdio.h>
#include <assert.h>
#include <stdbool.h>
#include <time.h>

int main(){
  time_t t2;
  srand((unsigned) time(&t2));
  int taille = 10;
  int g[taille][taille];
  
  int t[taille];
  for (int i = 0; i<taille; i++){
    t[i]=i;}

  for (int i = 0; i<taille; i++){
    for (int j = 0; i<taille; j++){
      t[i][j]=i;}
    }
  
  return 0;
}

