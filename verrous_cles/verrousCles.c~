#include <stdio.h>
#include <stdlib.h>


int test(int clé, double * array, size_t size){
  int i =0; int j = size; int emplacement = -1;
  while (i!=j){
    if (verrou(clé, array[i]) == 'ouvre'){
      if (i+1 == j){
	swap(array[i], array[i+1]);
	emplacement=i;}
      else { ++i;}
    }
    else if (verrou(clé,array[i]) == 'tourne'){
      ++i;
    }
    else{ //verrou(clé,array[i]) == 'bloque')
      swap(t[j],t[i]);
      --j;
    }
  }
  if (emplacement <= -1){
    return EXIT_FAILLURE;}
  return emplacement;
}
